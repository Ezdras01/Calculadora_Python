numero1=int(input("ingresa un número:"))
numero2=int(input("ingresa otro número:"))

eleccion= 0

while eleccion != 6:
    print("""Indique la operacion a realizar:
          1.- Suma
          2.- Resta 
          3.- Multiplicación    
          4.- División
          5.- Cambiar valores
          6.- Salir""")
    
    eleccion=int(input())
if eleccion == 1:
    print(" ")
    print("La suma de ",numero1,"+", numero2,"es:", numero1+numero2)    
